<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
       
       <bean id="res1" class="com.hexa.spring1.com.hexa1.spring.Result">
<property name="marks" value="100"/>
<property name="remarks" value="Pass"/>

</bean>
       

<bean id="book1" class="com.hexa.spring1.com.hexa1.spring.Book">
<constructor-arg index="0" value="101"/>
<constructor-arg index="1" value="DSA"/>
<constructor-arg index="2" value="800"/>



</bean>

<bean id="book2" class="com.hexa.spring1.com.hexa1.spring.Book">
<constructor-arg index="0" value="102"/>
<constructor-arg index="1" value="JAVA"/>
<constructor-arg index="2" value="1000"/>



</bean>



    <bean id="stud1" class="com.hexa.spring1.com.hexa1.spring.Student">
        <property name="roll" value="101" />
        <property name="name" value="Ajay" />
        <property name="fee" value="200" />
        <property name="res" ref="res1"/>
    </bean>

</beans>

